package javatpoint;

public class Country {
	private int id;
	private String name;
	public Country() {System.out.println("def cons");}  
	  
	public Country(int id) {this.id = id;}  
	  
	public Country(String name) {  this.name = name;}  
	  
	public Country(int id, String name) {  
	    this.id = id;  
	    this.name = name;  
	}  
	  
	void show(){  
	    System.out.println(id+" "+name);  
	}  
}
